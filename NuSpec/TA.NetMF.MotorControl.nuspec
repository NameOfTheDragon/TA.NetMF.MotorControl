<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>TA.NetMF.MotorControl</id>
        <version>0.0.0</version>
        <title>Motor control library for Netduino and .net Micro-Framework</title>
        <authors>Tim Long</authors>
        <owners>Tigra Astronomy</owners>
        <licenseUrl>https://github.com/NameOfTheDragon/TA.NetMF.MotorControl/blob/master/LICENSE.md</licenseUrl>
        <projectUrl>http://tigra-astronomy.com/stepper-motor-control-for-net-microframework</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>
          Contains motor drivers for stepper, D.C. and servo motors and works with popular arduino/netduino
          shields such as those from Sparkfun and Adafruit.
        </description>
        <summary>A collection of motor drivers, utilities and samples for Netduino targetting .Net MicroFramework 4.3+</summary>
        <releaseNotes>This project is still in flight. Anything may change at any time. Please refer to the project home page for further information.</releaseNotes>
        <copyright>Copyright © 2014 Tigra Astronomy, all rights reserved.</copyright>
        <language>en-GB</language>
        <tags>netduino,NetMF,MicroFramework,motor,stepper motor,c#</tags>
    </metadata>
    <files>
        <!-- Shield Agnostic Motor Control -->
        <file src="..\TA.NetMF.Motor\bin\Release\TA.NetMF.Motor.*" target="lib/netmf43/" />
        <file src="..\TA.NetMF.Motor\bin\Release\le\TA.NetMF.Motor.p*" target="lib/netmf43/le/" />
        <file src="..\TA.NetMF.Motor\bin\Release\be\TA.NetMF.Motor.p*" target="lib/netmf43/be/" />
        <!-- Sparkfun motor shield -->
        <file src="..\TA.NetMF.SparkfunArdumotoShield\bin\Release\TA.NetMF.ShieldDriver.SparkfunArdumoto.*" target="lib/netmf43/" />
        <file src="..\TA.NetMF.SparkfunArdumotoShield\bin\Release\le\TA.NetMF.ShieldDriver.SparkfunArdumoto.p*" target="lib/netmf43/le/" />
        <file src="..\TA.NetMF.SparkfunArdumotoShield\bin\Release\be\TA.NetMF.ShieldDriver.SparkfunArdumoto.p*" target="lib/netmf43/be/" />
        <!-- Adafruit V2 Motor Shield -->
        <file src="..\TA.NetMF.AdafruitMotorShieldV2\bin\Release\TA.NetMF.ShieldDriver.AdafruitV2.*" target="lib/netmf43/" />
        <file src="..\TA.NetMF.AdafruitMotorShieldV2\bin\Release\le\TA.NetMF.ShieldDriver.AdafruitV2.p*" target="lib/netmf43/le/" />
        <file src="..\TA.NetMF.AdafruitMotorShieldV2\bin\Release\be\TA.NetMF.ShieldDriver.AdafruitV2.p*" target="lib/netmf43/be/" />
        <!-- Adafruit V1 Motor Shield-->
        <file src="..\TA.NetMF.AdafruitMotorShield\bin\Release\TA.NetMF.ShieldDriver.AdafruitV1.*" target="lib/netmf43/" />
        <file src="..\TA.NetMF.AdafruitMotorShield\bin\Release\le\TA.NetMF.ShieldDriver.AdafruitV1.p*" target="lib/netmf43/le/" />
        <file src="..\TA.NetMF.AdafruitMotorShield\bin\Release\be\TA.NetMF.ShieldDriver.AdafruitV1.p*" target="lib/netmf43/be/" />
        <!-- LED Simulator Motor Shield -->
        <file src="..\TA.NetMF.MotorSimulator\bin\Release\TA.NetMF.ShieldDriver.Simulator.*" target="lib/netmf43/" />
        <file src="..\TA.NetMF.MotorSimulator\bin\Release\le\TA.NetMF.ShieldDriver.Simulator.p*" target="lib/netmf43/le/" />
        <file src="..\TA.NetMF.MotorSimulator\bin\Release\be\TA.NetMF.ShieldDriver.Simulator.p*" target="lib/netmf43/be/" />
    </files>
</package>